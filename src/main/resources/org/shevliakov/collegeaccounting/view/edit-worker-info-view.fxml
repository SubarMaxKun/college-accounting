<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>
<VBox alignment="CENTER_LEFT" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
  minWidth="-Infinity" prefHeight="600.0" prefWidth="500.0" spacing="10.0"
  xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1"
  fx:controller="org.shevliakov.collegeaccounting.controller.EditWorkerInfoController">
  <HBox alignment="BASELINE_RIGHT" prefWidth="200.0">
    <Text strokeType="OUTSIDE" strokeWidth="0.0" text="ПІБ">
      <HBox.margin>
        <Insets right="5.0"/>
      </HBox.margin>
    </Text>
    <TextField fx:id="fullNameTextField" prefWidth="200.0"/>
    <VBox.margin>
      <Insets left="40.0" right="40.0"/>
    </VBox.margin>
  </HBox>
  <HBox alignment="BASELINE_RIGHT" layoutX="10.0" layoutY="10.0" prefWidth="200.0">
    <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Звання">
      <HBox.margin>
        <Insets right="5.0"/>
      </HBox.margin>
    </Text>
    <ChoiceBox fx:id="rankChoiceBox" prefWidth="200.0"/>
    <VBox.margin>
      <Insets left="40.0" right="40.0"/>
    </VBox.margin>
  </HBox>
  <HBox alignment="BASELINE_RIGHT" layoutX="10.0" layoutY="110.0" prefWidth="200.0">
    <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Дата нарродження">
      <HBox.margin>
        <Insets right="5.0"/>
      </HBox.margin>
    </Text>
    <DatePicker fx:id="birthDateDatePicker" prefWidth="200.0"/>
    <VBox.margin>
      <Insets left="40.0" right="40.0"/>
    </VBox.margin>
  </HBox>
  <HBox alignment="BASELINE_RIGHT" prefWidth="200.0">
    <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Реєстраційний номер">
      <HBox.margin>
        <Insets right="5.0"/>
      </HBox.margin>
    </Text>
    <TextField fx:id="registrationNumberTextField" prefWidth="200.0"/>
    <VBox.margin>
      <Insets left="40.0" right="40.0"/>
    </VBox.margin>
  </HBox>
  <HBox alignment="BASELINE_RIGHT" prefWidth="200.0">
    <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Військово-облікова спеціальність">
      <HBox.margin>
        <Insets right="5.0"/>
      </HBox.margin>
    </Text>
    <TextField fx:id="militarySpecialtyTextField" prefWidth="200.0"/>
    <VBox.margin>
      <Insets left="40.0" right="40.0"/>
    </VBox.margin>
  </HBox>
  <HBox alignment="BASELINE_RIGHT" layoutX="10.0" layoutY="112.0" prefWidth="200.0">
    <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Склад підготовки">
      <HBox.margin>
        <Insets right="5.0"/>
      </HBox.margin>
    </Text>
    <ChoiceBox fx:id="trainingChoiceBox" prefWidth="200.0"/>
    <VBox.margin>
      <Insets left="40.0" right="40.0"/>
    </VBox.margin>
  </HBox>
  <HBox alignment="BASELINE_RIGHT" layoutX="10.0" layoutY="138.0" prefWidth="200.0">
    <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Категорія обліку">
      <HBox.margin>
        <Insets right="5.0"/>
      </HBox.margin>
    </Text>
    <TextField fx:id="accountingCategoryTextField" prefWidth="200.0"/>
    <VBox.margin>
      <Insets left="40.0" right="40.0"/>
    </VBox.margin>
  </HBox>
  <HBox alignment="BASELINE_RIGHT" layoutX="10.0" layoutY="164.0" prefWidth="200.0">
    <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Відомості про освіту та спеціальність">
      <HBox.margin>
        <Insets right="5.0"/>
      </HBox.margin>
    </Text>
    <TextField fx:id="degreeTextField" prefWidth="200.0"/>
    <VBox.margin>
      <Insets left="40.0" right="40.0"/>
    </VBox.margin>
  </HBox>
  <HBox alignment="BASELINE_RIGHT" layoutX="10.0" layoutY="189.0" prefWidth="200.0">
    <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Реквізити паспорта України">
      <HBox.margin>
        <Insets right="5.0"/>
      </HBox.margin>
    </Text>
    <TextArea fx:id="idInfoTextArea" prefWidth="200.0" wrapText="true"/>
    <VBox.margin>
      <Insets left="40.0" right="40.0"/>
    </VBox.margin>
  </HBox>
  <HBox alignment="CENTER" layoutX="50.0" layoutY="338.0" prefWidth="200.0" spacing="10.0">
    <Button fx:id="commitButton" mnemonicParsing="false" onAction="#onCommitButtonClicked"
      prefWidth="80.0" text="Оновити"/>
    <Button fx:id="deleteButton" mnemonicParsing="false" onAction="#onDeleteButtonClicked"
      prefWidth="80.0" text="Видалити" textFill="RED"/>
    <Button fx:id="cancelButton" layoutX="175.0" layoutY="10.0" mnemonicParsing="false"
      onAction="#onCancelButtonClicked" prefWidth="80.0" text="Скасувати"/>
  </HBox>
</VBox>
